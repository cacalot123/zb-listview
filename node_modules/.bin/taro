#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_@tarojs_cli@1.3.15@@tarojs/cli/bin/taro" "$@"
  ret=$?
else 
  node  "$basedir/../../../_@tarojs_cli@1.3.15@@tarojs/cli/bin/taro" "$@"
  ret=$?
fi
exit $ret
